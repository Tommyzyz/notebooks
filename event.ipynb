    "using  System.Timers;\n",
    "Boy boy = new();\n",
    "Timer timer = new();\n",
    "timer.Interval = 1000;\n",
    "timer.Elapsed +=boy.Actions;\n",
    "timer.Start();\n",
    "class Boy\n",
    "{\n",
    "    internal void Actions(object sender, ElapsedEventArgs e)\n",
    "    {\n",
    "        Console.WriteLine(\"Jump\");\n",
    "    }\n",
    "}"
   ]
