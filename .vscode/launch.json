{

    "version": "0.2.0",
  
  
    "configurations": [
  
  
      {
  
  
        "name": "(gdb) Launch",
  
  
        "type": "cppdbg",
  
  
        "request": "launch",
  
  
        "program": "${workspaceFolder}/C/main.exe",
  
  
        "args": [],
  
  
        "stopAtEntry": false,
  
  
        "cwd": "${workspaceFolder}",
  
  
        "environment": [],
  
  
        "externalConsole": true,
  
  
        "MIMode": "gdb",
  
  
        "preLaunchTask": "build",
  
  
        "miDebuggerPath": "C:/MinGW/bin/gdb.exe",
  
  
        "setupCommands": [
  
  
          {
  
  
            "description": "Enable pretty-printing for gdb",
  
  
            "text": "-enable-pretty-printing",
  
  
            "ignoreFailures": true
  
  
          }
  
  
        ],
  
  
        "logging": {
  
  
          "exceptions": true,
  
  
          "moduleLoad": true,
  
  
          "programOutput": true,
  
  
          "trace": true,
  
  
          "traceResponse": true
  
  
        }
  
  
      }
  
  
    ]
  
  
  }